package 변수및자료형;

import java.util.Scanner;

public class Exo1입출력 {

	public static void main(String[] args) {

		//주석 : 코드에 영향을 끼치지 않는 메모자 역활을함 
		//단축키 1) 글자크기 변경 : ctrl +-
	    //     2) 한 줄 삭제 : ctrl d
		//     4) 실행 단축키 : ctrl f11
		//     5)출력문 생성 단축키 : syso >ctrl space
		//     6)자동 import 단축키 : ctrl shift 
		
		
		//1. 출력문
		//>() 안쪽에 출력하고 싶은 구문 작성하면된다
		System.out.println("Hello World!");
		System.out.println("강우석");
		//출력문 생성 단축키 : syso >ctrl space
		
		//2. 입력문
		//2-1) 입력받는 도구 꺼내오기
		Scanner sc = new Scanner(System.in);
		System.out.println("글자를 입력하세요 >> ");
		//2-2) 입력받기
		sc.next();
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
