package 변수및자료형;

public class Ex02변수 {
	//main이라 작성후 strl 스페이스 하면 자동완성(main을 만들지 못한경우)

	public static void main(String[] args) {
		
		
		//변수 선언 
		//정수 데이터 4를 담을수 있는 num 변서 선언
		//자료형 변수명 = 값;
		  int num = 4;
	    // 변수를 사용하는 이유는 재사용하기 위해서 이다.  
		  System.out.println(num);
		  
		//1. 입력받는 도구 꺼내오기
		  Sanner sc = new Scanner(System.in);
		  System.out.println('num');
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
