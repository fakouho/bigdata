package 삼항연산자;

public class Ex01삼항연산자예제 {

	public static void main(String[] args) {
		//삼항연상자
		//조건문 ? 실행문1 : 실행문2
		//---> 조건문이 참이라면 실행문1 
		//---> 조건문이 거짓이라면 실행문2
		
		int num1 = 4;
		int num2 = 7;
		
		num1< num2 ? "num2가 더 크다" : "num1이 더 크다";
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
     
	}

}
